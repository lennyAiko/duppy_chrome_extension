{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "Duppy",
  "description": "Get page and SEO data",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["scripting", "tabs", "activeTab"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
